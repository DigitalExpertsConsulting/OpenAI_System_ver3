  <resources siena:authenticationProviderHref="#PowerAppAuth" base="https://asia-001.azure-apim.net/apim/office365groups">
    <resource path="/{connectionId}/v1.0/groups/{groupId}/members">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="groupId" type="xs:string" required="true">
        <doc title="드롭다운에서 그룹을 선택하거나 그룹 ID를 입력하세요." />
      </param>
      <method siena:requiresAuthentication="true" name="GET" id="ListGroupMembers" actionName="ListGroupMembers" visibility="advanced" siena:isDeprecated="false">
        <doc title="그룹 구성원 나열&#xA;이 작업은 지정된 그룹의 모든 구성원 목록과 이름, 직위, 이메일 등의 정보를 반환합니다. Top 매개 변수를 사용하여 최대 1,000개까지 항목을 쿼리할 수 있습니다. 1,000 개 이상의 값을 검색해야 하는 경우 설정에서 페이지 매김 기능을 켜고 임계값 제한을 제공하세요." />
        <request>
          <param style="query" name="$top" type="xs:int">
            <doc title="검색할 그룹 구성원 수(1~999, 기본값은 100)입니다." />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:ListGroupMembers_Response" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/v1.0/groups/{groupId}/members/$ref">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <param style="template" name="groupId" type="xs:string" required="true">
        <doc title="드롭다운에서 그룹을 선택하거나 그룹 ID를 입력하세요." />
      </param>
      <method siena:requiresAuthentication="true" name="POST" id="AddMemberToGroup" actionName="AddMemberToGroup" visibility="important" siena:isDeprecated="false">
        <doc title="그룹에 구성원 추가&#xA;이 작업은 UPN을 사용하여 O365 그룹에 구성원을 추가하는 데 사용됩니다." />
        <request>
          <param style="query" name="userUpn" type="xs:string" required="true">
            <doc title="사용자의 UPN(사용자 계정 이름)입니다." />
          </param>
        </request>
      </method>
    </resource>
    <resource path="/{connectionId}/v1.0/me/memberOf/$/microsoft.graph.group">
      <param style="template" name="connectionId" type="xs:string" required="true" />
